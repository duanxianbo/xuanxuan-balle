


page{
  align-content: center;
  justify-items: center;
  position: relative;
  display: flex;
  align-items: center;

}

.container {
  width: 100%;
	height: 100%;
}

.header {
	position: relative;
  width: 100%;
  align-content: center;
  justify-content: center;
	background: #c5c4c3;
	padding: 0rpx 10rpx;
	color: #000;
}
.header.hidden {
	/*-webkit-transform: translate3d(0,-290rpx,0);*/
	margin-top: -290rpx;
}
.header .logo {
	float: left;
	width: 160rpx;
	height: 160rpx;
	border: 2rpx solid #fff;
	border-radius: 50%;
	margin-right: 10px;
}

.header .name {
	padding-top: 20rpx;
	font-size: 36rpx;

}
.header .welcome {
	color: #333;
	font-size: 30rpx;
	width: 100%;
	line-height: 70rpx;
}
.header .follow {
	position: absolute;
	right: 20px;
	top: 50px;
	font-size: 30rpx;
	line-height: 2;
	padding: 0 0.7em;
	border: 1rpx solid;
	border-radius: 30rpx;
}
.header .line {
	clear: both;
	width: 100%;
	height: 1px;
	border-bottom: 1px dashed #000;
	padding-top: 20rpx;
}
.header .desc {
	color: #333;
	font-size: 30rpx;
	line-height: 40rpx;
	padding-top: 20rpx;
}

.content-container {

	height: 100%;

}
.classify-container {
	width: 180rpx;
	background: #efefef;
	height: 100%;
}
.classify-container .classify {
	padding: 50rpx 30rpx;
	text-align: center;
	border-bottom: 1px dashed #aaa;
}
.classify-container .classify.active {
	background: #fff;
}
.classify-container .classify .name {
	display: inline-block;
	font-size: 30rpx;
	color: #646464;
	line-height: 1.2;
	text-align: left;
	pointer-events: none;
}

.content-container .goods-container {
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-bottom: 100rpx;

}

.content-container .goods-container .goods {
  display: flex;
  width: 100%;
  height: 400rpx;
  flex-direction: row;
	/* position: fixed; */
  right: 20rpx;
	padding: 20rpx 30rpx;
	font-size: 36rpx;
	line-height: 60rpx;
	border-bottom: 1rpx solid #c0bfbf;

  
}



.content-container .goods-container .goods .item {
  width: 100%;
	height: 400rpx;
  display: flex;
  right: -100rpx;
  flex-direction: column;
  position: absolute;
  margin-bottom: 20rpx;

}



.content-container .goods-container .goods .item .name {
	color: #000;
  position: absolute;
  bottom: 110rpx;
	font-size: 36rpx;

}
.content-container .goods-container .goods .item .desc {
	color: #989898;
	font-size: 30rpx;
	line-height: 46rpx;
}

.goods-container .goods .item .methods{
  display: flex;
  flex-direction: row;
  position: absolute;
  bottom: 70rpx;
}

.goods-container .goods .item .methods1{
  display: flex;
  left: -35rpx;
  flex-direction: row;
  position: absolute;
  bottom: 10rpx;
}


.content-container .goods-container .goods .item .price {
  position: relative;
	color: #f45044;
	font-size: 30rpx;
}

.content-container .goods-container .goods .item .pic1{
  top: 15rpx;
  right: 152rpx;
  position: absolute;
  align-content: center;
  align-items: center;
  width: 85%;
  z-index: 1;
}

.content-container .goods-container .goods .item .pic2{
  display: flex;
  position: absolute;
  right: 140rpx;
  height: 420rpx;
  width: 90%;
  z-index: 0;
}



.goods-container .goods .item .addCart {
	width: 60rpx;
	height: 60rpx;
	/* background: #c5c4c3; */
	/* border-radius: 50%; */
}


.goods-container .goods .addCart image {
  left: 100rpx;
  position: fixed;
  width: 50rpx;
  height: 50rpx;
}


.btn{
  background-image: url("data:image/png;base64,");

  align-content: center;
  text-align: center;
  background-size: cover;
  align-items: center;
    border-radius: 25rpx;
  margin-top: 50rpx;
  width: 250rpx;
  height:60rpx;
  line-height: 60rpx;
  color: rgb(245, 240, 240);
  /* box-shadow: 2rpx 2rpx 2rpx 2rpx rgb(0, 0, 0); */
 }


.top{
top: 0rpx;
width: 100%;
display: flex;
position: absolute;
}
.top-image{
  width: 100%;
}

.txt{
  left: 20rpx;
  position: relative;
  color: gray;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}





