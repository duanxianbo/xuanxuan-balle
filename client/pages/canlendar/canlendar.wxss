/* pages/canlendar/canlendar.wxss */
/* 打卡日历 */
.all{
  margin-top: 20rpx;
}

.all .bar{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 30rpx 20rpx;
  padding: 10rpx;
}

.all .bar image{
  width: 50rpx;
  height: 50rpx;
}


.all .week{
  background-image: url("data:image/png;base64,");

  display: flex;
  justify-content: space-between;
  padding: 10rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  margin: 20rpx;
  width: 84%;
    border-radius: 25rpx;
  background-repeat: repeat-x;
  /* box-shadow: 2rpx 2rpx 2rpx 2rpx rgb(0, 0, 0); */
 }

.all .days{
  margin: 20rpx;
  padding: 10rpx;
  border-radius: 10px;
  background-color:white;

}

.all .columns{
  display: flex;
  flex-direction: column;
  justify-content: space-between; 
}

.all .columns .rows{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.all .columns .rows .cell{
  width: 84rpx;
  height: 88rpx;
  margin: 3rpx;
  text-align: center;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.count .daynumber{
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.count .daynumber .day{
  margin-top: 50rpx;
}

.count{
  margin: 20rpx;
  padding: 30rpx;
  display: flex;
  text-align: center;
  border-radius: 10px;
  flex-direction: ;
  justify-content: center;
  background-color: white;
  align-items: center;
}

.count .number{
  color: red;
  font-size: 60rpx;
  background-color: #fff;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 20rpx;
}

.count text{
  margin: 10rpx;
}


.imagesize{
  display: flex;
  justify-content: center;

}



.content{
  align-content: center;
  left:8rpx;
  position: relative;
  display: flex;
  width: 98%;
}


.imagesize image{
  height: 150rpx;
  width: 150rpx; 
}

.array_position{
   right:30rpx;
}

.block1{
  position: relative;
  display: flex;
  flex-direction: row;
  height: 90rpx;
  left:30rpx;
  font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif
}
.title_color{
  background-color: rgb(250, 205, 26)
}

.cont{
  position: relative;
  width: 100%;
  height: 150rpx;
  text-align: center;

}
/* 头像 */
.list_img{
  position: relative;
  top: 25%;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
/* button外层视图 */
.button{
  position: relative;
  text-align: center;
  top: 100rpx;
  width: 100%;
  height: 100rpx;
}
/* 按钮居中 */
.btn_sub{
  width: 100%;
  height: 50rpx;
  border: 0rpx;
  align-content: center;
  text-align: center;

}

.btn{
  background-image: url("data:image/png;base64,");

  align-content: center;
  position: relative;
  background-size: cover;
  display: flex;
  left:240rpx;
  text-align: center;
  justify-content: center;
  border-radius: 17rpx;
  align-items: center;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
  width: 250rpx;
  height:60rpx;
  color: rgb(245, 240, 240);
  /* box-shadow: 2rpx 2rpx 2rpx 2rpx rgb(0, 0, 0); */
 }
