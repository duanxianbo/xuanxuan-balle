/* pages/card/card.wxss */

.imagesize{
  display: flex;
  justify-content: center;

}



.content{
  align-content: center;
  left:8rpx;
  position: relative;
  display: flex;
  width: 98%;
}


.imagesize image{
  height: 150rpx;
  width: 150rpx; 
}

.array_position{
   right:30rpx;
}

.block1{
  position: relative;
  display: flex;
  flex-direction: row;
  height: 90rpx;
  left:30rpx;
  font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif
}
.title_color{
  background-color: rgb(250, 205, 26)
}

.cont{
  position: relative;
  width: 100%;
  height: 150rpx;
  text-align: center;

}
/* 头像 */
.list_img{
  position: relative;
  top: 25%;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
/* button外层视图 */
.button{
  position: relative;
  text-align: center;
  top: 100rpx;
  width: 100%;
  height: 100rpx;
}
/* 按钮居中 */
.btn_sub{
  width: 100%;
  height: 50rpx;
  border: 0rpx;
  align-content: center;
  text-align: center;

}


