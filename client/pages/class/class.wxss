/* pages/class/class.wxss */
page{
  font-size: 30rpx;
  font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif
}

.table{
  border: 2rpx solid #dadada;
  border-right: 0;
  border-bottom: 0;
  width:98%;
}

.tr{
  widows: 100%;
  display: flex;
  justify-content: space-between;
}

.th,.td{
  padding: 10rpx;
  border-bottom: 1rpx solid #dadada;
  border-right: 1rpx solid #dadada;
  text-align: center;
  width: 100%;
}

.th{
  font-weight: 400;
  
}